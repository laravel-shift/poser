{
    "name": "lukeraymonddowning/poser",
    "description": "A description for poser.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Luke Raymond Downing",
            "email": "lukeraymonddowning@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^6.0|^7.0|^8.0",
        "haydenpierce/class-finder": "^0.4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lukeraymonddowning\\Poser\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lukeraymonddowning\\Poser\\Tests\\": "./tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lukeraymonddowning\\Poser\\PoserServiceProvider"
            ]
        }
    }
}
